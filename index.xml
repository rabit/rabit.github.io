&lt;pre class=&#34;chroma&#34;&gt;&lt;code class=&#34;language-bash&#34; data-lang=&#34;bash&#34;&gt;git init
git remote add origin https://github.com/rabit/rabit.github.io.git
touch .gitkeep
git add .gitkeep
git commit -m &lt;span class=&#34;s2&#34;&gt;&amp;#34;Initial commit for User Pages&amp;#34;&lt;/span&gt;
git push -u origin master
git checkout --orphan hugo-code
git rm -rf .
&lt;span class=&#34;nb&#34;&gt;echo&lt;/span&gt; &lt;span class=&#34;s2&#34;&gt;&amp;#34;public/&amp;#34;&lt;/span&gt; &amp;gt;&amp;gt; .gitignore
git add .gitignore
git commit -m &lt;span class=&#34;s2&#34;&gt;&amp;#34;create new branch for hugo code&amp;#34;&lt;/span&gt;
git push -u origin hugo-code&lt;/code&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;
&lt;/div&gt;
&lt;/div&gt;
&lt;h3 id=&#34;hugo&#34;&gt;Hugo&lt;/h3&gt;
